{
  "name": "workflow",
  "version": "1.0.0",
  "description": "Fewder is a simple Front End Framework for developing Web App's using modules",
  "directories": {
    "doc": "docs"
  },
  "default": "src/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx parcel src/index.html && npm run json-watch",
    "build": "npx parcel build src/index.html && npm run json-watch",
    "clean:dev": "rm -rf .parcel-cache dist",
    "clean:git": "rm -rf .parcel-cache dist  node_modules",
    "json-watch": "json-server --watch src/data/toDo.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "http-server": "^13.0.2",
    "json-server": "^0.17.0",
    "parcel": "^2.0.0-rc.0",
    "sass": "^1.40.1"
  }
}
